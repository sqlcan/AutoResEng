    <DataSet Name="dsRequestsCompleted">
      <Query>
        <DataSourceName>dsMain</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@UserID">
            <Value>=Parameters!UserID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH RequestsCompleted AS (
  SELECT *, ROW_NUMBER() OVER (PARTITION BY AuthorizedUserID ORDER BY DateTimeRequestCompleted DESC) CompletedRank
    FROM vRestoreRequest
   WHERE DateTimeRequestCompleted IS NOT NULL)
SELECT *
  FROM RequestsCompleted
 WHERE CompletedRank &lt;= 5
    AND ((AuthorizedUserID = @UserID) OR (@UserID = -1))
ORDER BY DateTimeRequestCompleted DESC</CommandText>
      </Query>
