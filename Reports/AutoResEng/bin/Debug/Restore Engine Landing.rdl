  <DataSets>
    <DataSet Name="dsRequestStatus">
      <Query>
        <DataSourceName>dsMain</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@UserID">
            <Value>=Parameters!UserID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH RequestsByStatus AS (SELECT        CASE WHEN (DateTimeRequestCompleted IS NULL) THEN 'Pending' WHEN IsSuccessful = 0 THEN 'Failed' ELSE 'Successful' END AS RequestStatus
                                                               FROM            RestoreRequest
                                                               WHERE        ((AuthorizedUserID = @UserID) OR (@UserID = -1)))
    SELECT        RequestStatus, COUNT(*) AS RequestCounts
     FROM            RequestsByStatus
     GROUP BY RequestStatus</CommandText>
      </Query>
      <Fields>
        <Field Name="RequestStatus">
          <DataField>RequestStatus</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="RequestCounts">
          <DataField>RequestCounts</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="pUserID">
      <Query>
        <DataSourceName>dsMain</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@UserName">
            <Value>=Parameters!UserName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        CASE WHEN (IsSysAdmin = 1) THEN - 1 ELSE ID END AS ID
FROM            AuthorizedUsers
WHERE        (IsEnabled = 1) AND (UserName = @UserName)</CommandText>
      </Query>
      <Fields>
        <Field Name="ID">
          <DataField>ID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
